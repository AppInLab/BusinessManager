<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="BusinessEngine.Models" assembly="BusinessEngine">
  <class name="Client" table="Client" lazy="false">
    <id name="Id">
      <column name="Id"/>
      <generator class="native"/>
    </id>

    <property name="DateCreation">
      <column name="DateCreation"/>
    </property>

    <property name="DateModification">
      <column name="DateModification"/>
    </property>
    
    <property name="NomComplet">
      <column name="NomComplet"/>
    </property>
    
    <property name="Adresse">
      <column name="Adresse"/>
    </property>

    <property name="Contacts">
      <column name="Contacts"/>
    </property>

    <property name="Doit">
      <column name="Doit"/>
    </property>

    <property name="Avoir">
      <column name="Avoir"/>
    </property>
  </class>
</hibernate-mapping>